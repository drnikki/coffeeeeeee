
<div id="StoreConfigs"></div>
<script type="text/javascript">
  $(function() {
    // Blog is the app name
    window.router = new Barista.Routers.StoreConfigsRouter({store_configs: <%= @store_configs.to_json.html_safe %>});
    Backbone.history.start();
  });
</script>
<h1>Listing store_configs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @store_configs.each do |store_config| %>
  <tr>
    <td><%= store_config.name %></td>
    <td><%= store_config.value %></td>
    <td><%= link_to 'Show', store_config %></td>
    <td><%= link_to 'Edit', edit_store_config_path(store_config) %></td>
    <td><%= link_to 'Destroy', store_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store config', new_store_config_path %>
